import java.util.Scanner;
public class Program2 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.println("Enter a number R1");
        int R1 = in.nextInt();
        System.out.println("Enter a number R2");
        int R2 = in.nextInt();
        double outerRadiusR1 = R1 * R1 * Math.PI;
        double innerRadiusR2 = R2 * R2 * Math.PI;
        double areaOfCircle = outerRadiusR1 - innerRadiusR2;
        if (R1 > R2) {
            System.out.println("The area of the circle is " + areaOfCircle);
        } else{
            System.out.println("Conditions are not met");
        }
    }
}
