public class Program3 {
public static void main(String[] args) {
        double R1 = 8;
        double R2 = 5;
        double outerRadiusR1 = R1 * R1 * Math.PI;
        double innerRadiusR2 = R2 * R2 * Math.PI;
        double areaOfCircle = outerRadiusR1 - innerRadiusR2;

        System.out.println("The area of the circle is " + areaOfCircle);
        }
}
